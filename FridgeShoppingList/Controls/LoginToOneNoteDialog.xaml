<lcarsModal:LcarsModalDialog
    x:Class="FridgeShoppingList.Controls.LoginToOneNoteDialog"
    xmlns:lcarsModal="using:FridgeShoppingList.Controls.LcarsModalDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FridgeShoppingList.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="ONENOTE LOGIN"
    PrimaryButtonClick="LcarsModalDialog_PrimaryButtonClick"
    PrimaryButtonText="CLOSE"
    IsSecondaryButtonEnabled="False"
    SecondaryButtonVisibility="Collapsed">
    
    <WebView Height="330" Width="400"
             HorizontalAlignment="Stretch"                                   
             NavigationStarting="WebView_NavigationStarting"
             Source="{x:Bind ViewModel.WebViewUrl, Mode=TwoWay}" 
             VerticalAlignment="Stretch"/>
</lcarsModal:LcarsModalDialog>
